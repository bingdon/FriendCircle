<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="Title_style" parent="@android:style/Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="Back_style" parent="@android:style/Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15sp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/left_sec</item>
    </style>
    
    
    <style name="Small_style" parent="@android:style/Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    
    <style name="Name_style" parent="@android:style/Widget.TextView">
        <item name="android:textColor">@color/deepskyblue</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="Content_txt_style" parent="@android:style/Widget.TextView">
        <item name="android:textColor">@color/gray</item>
    </style>
    
    <style name="Bingnotitle" parent="AppTheme">
         <item name="android:windowNoTitle">true</item>  
    </style>
    
    
    <style name="defineCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
     <item name="android:button">@drawable/check_box_sex</item>
   </style> 
   
     <style name="MRadioButton" parent="@android:style/Widget.CompoundButton.RadioButton">
     <item name="android:button">@drawable/check_box_sex</item>
   </style> 
    

</resources>